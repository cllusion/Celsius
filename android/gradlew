#!/usr/bin/env sh
###############################################################################
# Gradle start up script for UN*X
###############################################################################
set -e

PRG="$0"
while [ -h "$PRG" ] ; do
  ls=`ls -ld "$PRG"`
  link=`expr "$ls" : '.*-> \(.*\)$'`
  if expr "$link" : '/.*' > /dev/null; then
    PRG="$link"
  else
    PRG=`dirname "$PRG"`"/"`expr "$link" : '\(.*\)$'`
  fi
done

PRGDIR=`dirname "$PRG"`
exec  "${JAVA_HOME:-java}" -classpath "$PRGDIR/gradle/wrapper/gradle-wrapper.jar" org.gradle.wrapper.GradleWrapperMain "$@"
